{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/vue3-essential",
		"@vue/typescript/recommended",
		"@nuxtjs/eslint-config-typescript",
		"@vue/eslint-config-prettier"
	],
	"overrides": [],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module",
		"parser": "@typescript-eslint/parser"
	},
	"plugins": ["vue", "@typescript-eslint"],
	"rules": {}
}
